{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@18"
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node@18"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ],
  "buildCommand": "npm run build"
}
